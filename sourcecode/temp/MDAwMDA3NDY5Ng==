		});
		function searchOnclick(_param){ 
			//location.href="http://search.esgcc.com.cn/managerSearch/showSearchPage?q="+doubleBase64Encode($.trim(_param));	
			window.open($("#hideSearchArea").val()+ "?q="+doubleBase64Encode($.trim(_param)));	
		}
		//双重base64编码
		function doubleBase64Encode(ta){
			return Base64Utils.base64Encode(Base64Utils.base64Encode(ta));
		}
	</script>
